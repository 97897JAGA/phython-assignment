def firstUniqChar(s):
    """
    Find the index of the first non-repeating character in the string.
    """
    char_counts = {}  
     for char in s:
        char_counts[char] = char_counts.get(char, 0) + 1
    for i in range(len(s)):
        if char_counts[s[i]] == 1:
            return i

    return -1  
